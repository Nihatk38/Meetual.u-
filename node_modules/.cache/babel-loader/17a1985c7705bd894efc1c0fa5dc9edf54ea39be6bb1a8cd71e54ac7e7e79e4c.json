{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { onMounted, ref } from \"vue\";\nimport FullCalendar from '@fullcalendar/vue3';\nimport dayGridPlugin from '@fullcalendar/daygrid';\nimport trLocale from '@fullcalendar/core/locales/tr';\nimport CreateBookingDialog from \"@/components/BookingHub/components/CreateBookingDialog.vue\";\nimport bookingData from \"@/auxiliary/userBookingInfos.json\";\nexport default {\n  name: 'BookingHubList',\n  components: {\n    FullCalendar,\n    CreateBookingDialog\n  },\n  setup() {\n    const status = sessionStorage.getItem('user.Status');\n    const consultationParticipantList = ref([]);\n    const selectedUser = ref();\n    const dialog = ref(false);\n    const calendarOptions = ref({\n      plugins: [dayGridPlugin],\n      initialView: 'dayGridMonth',\n      locale: trLocale,\n      headerToolbar: {\n        left: 'prev,next today',\n        center: 'title',\n        right: 'myCustomButton'\n      },\n      customButtons: {\n        myCustomButton: {\n          text: 'Yeni Randevu Oluştur',\n          click() {\n            openDialog();\n          }\n        }\n      },\n      events: []\n    });\n    const calendarRef = ref(null);\n    const getBookingData = () => {\n      const data = bookingData.bookingList;\n      const userName = sessionStorage.getItem('user.UserName');\n      if (status == 2) {\n        data.forEach(booking => {\n          if (booking.AdvisorName == userName && booking.IsConfirm) calendarOptions.value.events.push(booking);\n        });\n      } else if (status == 3) {\n        data.forEach(booking => {\n          if (booking.ClientName == userName && booking.IsConfirm) calendarOptions.value.events.push(booking);\n        });\n      }\n      prepareTheData();\n    };\n    const prepareTheData = () => {\n      calendarOptions.value.events.forEach(event => {\n        const start = new Date(event.start);\n        const end = new Date(event.end);\n        const name = status == 2 ? event.ClientName : event.AdvisorName;\n        event.title = name + \"-> \" + start.toLocaleTimeString('tr-TR', {\n          hour: '2-digit',\n          minute: '2-digit'\n        }) + \"-\" + end.toLocaleTimeString('tr-TR', {\n          hour: '2-digit',\n          minute: '2-digit'\n        });\n      });\n      calendarRef.value.getApi().refetchEvents();\n    };\n    const addBooking = booking => {\n      calendarOptions.value.events.push(booking);\n      prepareTheData();\n    };\n    const openDialog = () => {\n      dialog.value = true;\n    };\n    const closeDialog = () => {\n      dialog.value = false;\n    };\n    onMounted(async () => {\n      await getBookingData();\n    });\n    return {\n      consultationParticipantList,\n      selectedUser,\n      dialog,\n      calendarOptions,\n      calendarRef,\n      openDialog,\n      closeDialog,\n      addBooking\n    };\n  }\n};","map":{"version":3,"names":["onMounted","ref","FullCalendar","dayGridPlugin","trLocale","CreateBookingDialog","bookingData","name","components","setup","status","sessionStorage","getItem","consultationParticipantList","selectedUser","dialog","calendarOptions","plugins","initialView","locale","headerToolbar","left","center","right","customButtons","myCustomButton","text","click","openDialog","events","calendarRef","getBookingData","data","bookingList","userName","forEach","booking","AdvisorName","IsConfirm","value","push","ClientName","prepareTheData","event","start","Date","end","title","toLocaleTimeString","hour","minute","getApi","refetchEvents","addBooking","closeDialog"],"sources":["C:\\Users\\tkara.KARAHAN\\OneDrive\\Masaüstü\\Yeni klasör\\meetual.ui\\src\\components\\BookingHub\\views\\BookingHubList.vue"],"sourcesContent":["<template>\r\n  <div class=\"h-full mt-3 \">\r\n    <FullCalendar ref=\"calendarRef\" :options=\"calendarOptions\"/>\r\n\r\n  </div>\r\n  <CreateBookingDialog v-if=\"dialog\" :closeDialog=\"closeDialog\" @bookingInfo=\"addBooking\"/>\r\n</template>\r\n\r\n<script>\r\nimport {onMounted, ref} from \"vue\";\r\n\r\nimport FullCalendar from '@fullcalendar/vue3';\r\nimport dayGridPlugin from '@fullcalendar/daygrid';\r\nimport trLocale from '@fullcalendar/core/locales/tr';\r\n\r\nimport CreateBookingDialog from \"@/components/BookingHub/components/CreateBookingDialog.vue\";\r\n\r\nimport bookingData from \"@/auxiliary/userBookingInfos.json\";\r\n\r\n\r\nexport default {\r\n  name: 'BookingHubList',\r\n  components: {FullCalendar, CreateBookingDialog},\r\n  setup() {\r\n    const status = sessionStorage.getItem('user.Status');\r\n    const consultationParticipantList = ref([]);\r\n    const selectedUser = ref()\r\n    const dialog = ref(false)\r\n    const calendarOptions = ref({\r\n      plugins: [dayGridPlugin],\r\n      initialView: 'dayGridMonth',\r\n      locale: trLocale,\r\n      headerToolbar: {\r\n        left: 'prev,next today',\r\n        center: 'title',\r\n        right: 'myCustomButton'\r\n      },\r\n      customButtons: {\r\n        myCustomButton: {\r\n          text: 'Yeni Randevu Oluştur',\r\n          click() {\r\n            openDialog()\r\n          }\r\n        }\r\n      },\r\n      events: [],\r\n    })\r\n    const calendarRef = ref(null);\r\n\r\n    const getBookingData = () => {\r\n      const data = bookingData.bookingList;\r\n      const userName = sessionStorage.getItem('user.UserName');\r\n      if (status == 2) {\r\n        data.forEach(booking => {\r\n          if (booking.AdvisorName == userName && booking.IsConfirm)\r\n            calendarOptions.value.events.push(booking)\r\n        })\r\n      } else if (status == 3) {\r\n        data.forEach(booking => {\r\n          if (booking.ClientName == userName && booking.IsConfirm)\r\n            calendarOptions.value.events.push(booking)\r\n        })\r\n      }\r\n      prepareTheData()\r\n    }\r\n    const prepareTheData = () => {\r\n      calendarOptions.value.events.forEach(event => {\r\n        const start = new Date(event.start);\r\n        const end = new Date(event.end);\r\n        const name = status == 2 ? event.ClientName : event.AdvisorName\r\n\r\n        event.title = name + \"-> \" + start.toLocaleTimeString('tr-TR', {\r\n          hour: '2-digit',\r\n          minute: '2-digit'\r\n        }) + \"-\" + end.toLocaleTimeString('tr-TR', {\r\n          hour: '2-digit',\r\n          minute: '2-digit'\r\n        })\r\n      })\r\n      calendarRef.value.getApi().refetchEvents();\r\n    }\r\n\r\n    const addBooking = (booking) => {\r\n      calendarOptions.value.events.push(booking);\r\n      prepareTheData()\r\n    }\r\n    const openDialog = () => {\r\n      dialog.value = true;\r\n    }\r\n\r\n    const closeDialog = () => {\r\n      dialog.value = false;\r\n    }\r\n\r\n\r\n    onMounted(async () => {\r\n      await getBookingData()\r\n    })\r\n    return {\r\n      consultationParticipantList, selectedUser, dialog, calendarOptions, calendarRef,\r\n      openDialog, closeDialog, addBooking\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":";AASA,SAAQA,SAAS,EAAEC,GAAG,QAAO,KAAK;AAElC,OAAOC,YAAW,MAAO,oBAAoB;AAC7C,OAAOC,aAAY,MAAO,uBAAuB;AACjD,OAAOC,QAAO,MAAO,+BAA+B;AAEpD,OAAOC,mBAAkB,MAAO,4DAA4D;AAE5F,OAAOC,WAAU,MAAO,mCAAmC;AAG3D,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,UAAU,EAAE;IAACN,YAAY;IAAEG;EAAmB,CAAC;EAC/CI,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIC,cAAc,CAACC,OAAO,CAAC,aAAa,CAAC;IACpD,MAAMC,2BAA0B,GAAIZ,GAAG,CAAC,EAAE,CAAC;IAC3C,MAAMa,YAAW,GAAIb,GAAG,CAAC;IACzB,MAAMc,MAAK,GAAId,GAAG,CAAC,KAAK;IACxB,MAAMe,eAAc,GAAIf,GAAG,CAAC;MAC1BgB,OAAO,EAAE,CAACd,aAAa,CAAC;MACxBe,WAAW,EAAE,cAAc;MAC3BC,MAAM,EAAEf,QAAQ;MAChBgB,aAAa,EAAE;QACbC,IAAI,EAAE,iBAAiB;QACvBC,MAAM,EAAE,OAAO;QACfC,KAAK,EAAE;MACT,CAAC;MACDC,aAAa,EAAE;QACbC,cAAc,EAAE;UACdC,IAAI,EAAE,sBAAsB;UAC5BC,KAAKA,CAAA,EAAG;YACNC,UAAU,CAAC;UACb;QACF;MACF,CAAC;MACDC,MAAM,EAAE;IACV,CAAC;IACD,MAAMC,WAAU,GAAI7B,GAAG,CAAC,IAAI,CAAC;IAE7B,MAAM8B,cAAa,GAAIA,CAAA,KAAM;MAC3B,MAAMC,IAAG,GAAI1B,WAAW,CAAC2B,WAAW;MACpC,MAAMC,QAAO,GAAIvB,cAAc,CAACC,OAAO,CAAC,eAAe,CAAC;MACxD,I