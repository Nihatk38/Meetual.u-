{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { onMounted, ref } from \"vue\";\nimport FullCalendar from '@fullcalendar/vue3';\nimport dayGridPlugin from '@fullcalendar/daygrid';\nimport trLocale from '@fullcalendar/core/locales/tr';\nimport EditBookingDialog from \"@/components/BookingHub/components/editBookingDialog.vue\";\nimport bookingData from \"@/auxiliary/userBookingInfos.json\";\nexport default {\n  name: 'BookingHubList',\n  components: {\n    FullCalendar,\n    EditBookingDialog\n  },\n  setup() {\n    const consultationParticipantList = ref([]);\n    const selectedUser = ref();\n    const dialog = ref(false);\n    const menu = ref();\n    const calendarOptions = ref({\n      plugins: [dayGridPlugin],\n      initialView: 'dayGridMonth',\n      locale: trLocale,\n      events: []\n    });\n    const calendarApi = this.$refs.fullCalendar.getApi();\n    const prepareTheData = () => {\n      const data = bookingData.bookingList;\n      const status = sessionStorage.getItem('user.Status');\n      const userName = sessionStorage.getItem('user.UserName');\n      console.log(calendarApi);\n      if (status == 2) {\n        data.forEach(booking => {\n          if (booking.AdvisorName == userName) calendarOptions.value.events.push(booking);\n        });\n      } else if (status == 3) {\n        data.forEach(booking => {\n          if (booking.ClientName == userName) calendarOptions.value.events.push(booking);\n        });\n      }\n      calendarOptions.value.events.forEach(event => {\n        const start = new Date(event.start);\n        const end = new Date(event.end);\n        event.title = \"-> \" + start.toLocaleTimeString('tr-TR', {\n          hour: '2-digit',\n          minute: '2-digit'\n        }) + \"-\" + end.toLocaleTimeString('tr-TR', {\n          hour: '2-digit',\n          minute: '2-digit'\n        });\n      });\n      console.log(\"calendarOptions.value.events\", calendarOptions.value.events);\n    };\n    const menuModel = ref([{\n      label: \"Randevu Oluştur\",\n      icon: \"pi pi-plus\",\n      command: () => {\n        openDialog();\n      }\n    }]);\n    const openDialog = () => {\n      dialog.value = true;\n    };\n    const closeDialog = () => {\n      dialog.value = false;\n    };\n    const onRowContextMenu = event => {\n      menu.value.show(event.originalEvent);\n    };\n    onMounted(async () => {\n      await prepareTheData();\n    });\n    return {\n      consultationParticipantList,\n      selectedUser,\n      dialog,\n      menuModel,\n      menu,\n      calendarOptions,\n      openDialog,\n      closeDialog,\n      onRowContextMenu\n    };\n  }\n};","map":{"version":3,"names":["onMounted","ref","FullCalendar","dayGridPlugin","trLocale","EditBookingDialog","bookingData","name","components","setup","consultationParticipantList","selectedUser","dialog","menu","calendarOptions","plugins","initialView","locale","events","calendarApi","$refs","fullCalendar","getApi","prepareTheData","data","bookingList","status","sessionStorage","getItem","userName","console","log","forEach","booking","AdvisorName","value","push","ClientName","event","start","Date","end","title","toLocaleTimeString","hour","minute","menuModel","label","icon","command","openDialog","closeDialog","onRowContextMenu","show","originalEvent"],"sources":["C:\\Users\\tkara.KARAHAN\\OneDrive\\Masaüstü\\Yeni klasör\\meetual.ui\\src\\components\\BookingHub\\views\\BookingHubList.vue"],"sourcesContent":["<template>\r\n  <div class=\"h-full \">\r\n    <FullCalendar :options=\"calendarOptions\"/>\r\n  </div>\r\n  <EditBookingDialog v-if=\"dialog\" :closeDialog=\"closeDialog\"  />\r\n</template>\r\n\r\n<script>\r\nimport {onMounted, ref} from \"vue\";\r\n\r\nimport FullCalendar from '@fullcalendar/vue3';\r\nimport dayGridPlugin from '@fullcalendar/daygrid';\r\nimport trLocale from '@fullcalendar/core/locales/tr';\r\n\r\nimport EditBookingDialog from \"@/components/BookingHub/components/editBookingDialog.vue\";\r\n