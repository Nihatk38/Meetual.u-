{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { onMounted, ref } from \"vue\";\nimport FullCalendar from '@fullcalendar/vue3';\nimport dayGridPlugin from '@fullcalendar/daygrid';\nimport trLocale from '@fullcalendar/core/locales/tr';\nimport CreateBookingDialog from \"@/components/BookingHub/components/CreateBookingDialog.vue\";\nimport bookingData from \"@/auxiliary/userBookingInfos.json\";\nexport default {\n  name: 'BookingHubList',\n  components: {\n    FullCalendar,\n    CreateBookingDialog\n  },\n  setup() {\n    const consultationParticipantList = ref([]);\n    const selectedUser = ref();\n    const dialog = ref(false);\n    const calendarOptions = ref({\n      plugins: [dayGridPlugin],\n      initialView: 'dayGridMonth',\n      locale: trLocale,\n      headerToolbar: {\n        left: 'prev,next today',\n        center: 'title',\n        right: 'myCustomButton'\n      },\n      customButtons: {\n        myCustomButton: {\n          text: 'Yeni Randevu Oluştur',\n          click() {\n            openDialog();\n          }\n        }\n      },\n      events: []\n    });\n    const calendarRef = ref(null);\n    const prepareTheData = () => {\n      const data = bookingData.bookingList;\n      const status = sessionStorage.getItem('user.Status');\n      const userName = sessionStorage.getItem('user.UserName');\n      console.log(status);\n      if (status == 2) {\n        data.forEach(booking => {\n          if (booking.AdvisorName == userName) calendarOptions.value.events.push(booking);\n        });\n      } else if (status == 3) {\n        data.forEach(booking => {\n          if (booking.ClientName == userName) calendarOptions.value.events.push(booking);\n        });\n      }\n      calendarOptions.value.events.forEach(event => {\n        const start = new Date(event.start);\n        const end = new Date(event.end);\n        const name = status == 2 ? event.ClientName : event.AdvisorName;\n        event.title = name + \"-> \" + start.toLocaleTimeString('tr-TR', {\n          hour: '2-digit',\n          minute: '2-digit'\n        }) + \"-\" + end.toLocaleTimeString('tr-TR', {\n          hour: '2-digit',\n          minute: '2-digit'\n        });\n      });\n      calendarRef.value.getApi().refetchEvents();\n    };\n    const openDialog = () => {\n      dialog.value = true;\n    };\n    const closeDialog = () => {\n      dialog.value = false;\n    };\n    onMounted(async () => {\n      await prepareTheData();\n    });\n    return {\n      consultationParticipantList,\n      selectedUser,\n      dialog,\n      calendarOptions,\n      calendarRef,\n      openDialog,\n      closeDialog\n    };\n  }\n};","map":{"version":3,"names":["onMounted","ref","FullCalendar","dayGridPlugin","trLocale","CreateBookingDialog","bookingData","name","components","setup","consultationParticipantList","selectedUser","dialog","calendarOptions","plugins","initialView","locale","headerToolbar","left","center","right","customButtons","myCustomButton","text","click","openDialog","events","calendarRef","prepareTheData","data","bookingList","status","sessionStorage","getItem","userName","console","log","forEach","booking","AdvisorName","value","push","ClientName","event","start","Date","end","title","toLocaleTimeString","hour","minute","getApi","refetchEvents","closeDialog"],"sources":["C:\\Users\\tkara.KARAHAN\\OneDrive\\Masaüstü\\Yeni klasör\\meetual.ui\\src\\components\\BookingHub\\views\\BookingHubList.vue"],"sourcesContent":["<template>\r\n  <div class=\"h-full mt-3 \">\r\n    <FullCalendar ref=\"calendarRef\" :options=\"calendarOptions\" />\r\n\r\n  </div>\r\n  <CreateBookingDialog v-if=\"dialog\" :closeDialog=\"closeDialog\" @/>\r\n</template>\r\n\r\n<script>\r\nimport {onMounted, ref} from \"vue\";\r\n\r\nimport FullCalendar from '@fullcalendar/vue3';\r\nimport dayGridPlugin from '@fullcalendar/daygrid';\r\nimport trLocale from '@fullcalendar/core/locales/tr';\r\n\r\nimport CreateBookingDialog from \"@/components/BookingHub/components/CreateBookingDialog.vue\";\r\n\r\nimport bookingData from \"@/auxiliary/userBookingInfos.json\";\r\n\r\n\r\nexport default {\r\n  name: 'BookingHubList',\r\n  components: {FullCalendar, CreateBookingDialog},\r\n  setup() {\r\n    const consultationParticipantList = ref([]);\r\n    const selectedUser = ref()\r\n    const dialog = ref(false)\r\n    const calendarOptions = ref({\r\n      plugins: [dayGridPlugin],\r\n      initialView: 'dayGridMonth',\r\n      locale: trLocale,\r\n      headerToolbar: {\r\n        left: 'prev,next today',\r\n        center: 'title',\r\n        right: 'myCustomButton'\r\n      },\r\n      customButtons: {\r\n        myCustomButton: {\r\n          text: 'Yeni Randevu Oluştur',\r\n          click() {\r\n            openDialog()\r\n          }\r\n        }\r\n      },\r\n      events: [],\r\n    })\r\n    const calendarRef = ref(null);\r\n    const prepareTheData = () => {\r\n      const data = bookingData.bookingList;\r\n      const status = sessionStorage.getItem('user.Status');\r\n      const userName = sessionStorage.getItem('user.UserName');\r\n      console.log(status)\r\n      if (status == 2) {\r\n        data.forEach(booking => {\r\n          if (booking.AdvisorName == userName)\r\n            calendarOptions.value.events.push(booking)\r\n        })\r\n      } else if (status == 3) {\r\n        data.forEach(booking => {\r\n          if (booking.ClientName == userName)\r\n            calendarOptions.value.events.push(booking)\r\n        })\r\n      }\r\n      calendarOptions.value.events.forEach(event => {\r\n        const start = new Date(event.start);\r\n        const end = new Date(event.end);\r\n        const name =status == 2 ? event.ClientName : event.AdvisorName\r\n\r\n        event.title =name + \"-> \" + start.toLocaleTimeString('tr-TR', {\r\n          hour: '2-digit',\r\n          minute: '2-digit'\r\n        }) + \"-\" + end.toLocaleTimeString('tr-TR', {\r\n          hour: '2-digit',\r\n          minute: '2-digit'\r\n        })\r\n      })\r\n      calendarRef.value.getApi().refetchEvents();\r\n    }\r\n\r\n    const openDialog = () => {\r\n      dialog.value = true;\r\n    }\r\n\r\n    const closeDialog = () => {\r\n      dialog.value = false;\r\n    }\r\n\r\n\r\n\r\n    onMounted(async () => {\r\n      await prepareTheData()\r\n    })\r\n    return {\r\n      consultationParticipantList, selectedUser, dialog,  calendarOptions, calendarRef,\r\n      openDialog, closeDialog,\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":";AASA,SAAQA,SAAS,EAAEC,GAAG,QAAO,KAAK;AAElC,OAAOC,YAAW,MAAO,oBAAoB;AAC7C,OAAOC,aAAY,MAAO,uBAAuB;AACjD,OAAOC,QAAO,MAAO,+BAA+B;AAEpD,OAAOC,mBAAkB,MAAO,4DAA4D;AAE5F,OAAOC,WAAU,MAAO,mCAAmC;AAG3D,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,UAAU,EAAE;IAACN,YAAY;IAAEG;EAAmB,CAAC;EAC/CI,KAAKA,CAAA,EAAG;IACN,MAAMC,2BAA0B,GAAIT,GAAG,CAAC,EAAE,CAAC;IAC3C,MAAMU,YAAW,GAAIV,GAAG,CAAC;IACzB,MAAMW,MAAK,GAAIX,GAAG,CAAC,KAAK;IACxB,MAAMY,eAAc,GAAIZ,GAAG,CAAC;MAC1Ba,OAAO,EAAE,CAACX,aAAa,CAAC;MACxBY,WAAW,EAAE,cAAc;MAC3BC,MAAM,EAAEZ,QAAQ;MAChBa,aAAa,EAAE;QACbC,IAAI,EAAE,iBAAiB;QACvBC,MAAM,EAAE,OAAO;QACfC,KAAK,EAAE;MACT,CAAC;MACDC,aAAa,EAAE;QACbC,cAAc,EAAE;UACdC,IAAI,EAAE,sBAAsB;UAC5BC,KAAKA,CAAA,EAAG;YACNC,UAAU,CAAC;UACb;QACF;MACF,CAAC;MACDC,MAAM,EAAE;IACV,CAAC;IACD,MAAMC,WAAU,GAAI1B,GAAG,CAAC,IAAI,CAAC;IAC7B,MAAM2B,cAAa,GAAIA,CAAA,KAAM;MAC3B,MAAMC,IAAG,GAAIvB,WAAW,CAACwB,WAAW;MACpC,MAAMC,MAAK,GAAIC,cAAc,CAACC,OAAO,CAAC,aAAa,CAAC;MACpD,MAAMC,QAAO,GAAIF,cAAc,CAACC,OAAO,CAAC,eAAe,CAAC;MACxDE,OAAO,CAACC,GAAG,CAACL,MAAM;MAClB,IAAIA,MAAK,IAAK,CAAC,EAAE;QACfF,IAAI,CAACQ,OAAO,CAACC,OAAM,IAAK;UACtB,IAAIA,OAAO,CAACC,WAAU,IAAKL,QAAQ,EACjCrB,eAAe,CAAC2B,KAAK,CAACd,MAAM,CAACe,IAAI,CAACH,OAAO;QAC7C,CAAC;MACH,OAAO,IAAIP,MAAK,IAAK,CAAC,EAAE;QACtBF,IAAI,CAACQ,OAAO,CAACC,OAAM,IAAK;UACtB,IAAIA,OAAO,CAACI,UAAS,IAAKR,QAAQ,EAChCrB,eAAe,CAAC2B,KAAK,CAACd,MAAM,CAACe,IAAI,CAACH,OAAO;QAC7C,CAAC;MACH;MACAzB,eAAe,CAAC2B,KAAK,CAACd,MAAM,CAACW,OAAO,CAACM,KAAI,IAAK;QAC5C,MAAMC,KAAI,GAAI,IAAIC,IAAI,CAACF,KAAK,C